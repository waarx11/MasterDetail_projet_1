                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <StackPanel Grid.Column="0"/>
            <Button Content="Contact" MinWidth="60"  MinHeight="30" Grid.Column="1" Margin="0,10,20,10"
